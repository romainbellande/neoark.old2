    "@types/date-fns": "^2.6.0",
    "@types/jest": "^24.0.0",
    "@types/logrocket-react": "^3.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.3",
    "@types/yup": "^0.26.27",
    "d3-format": "^1.4.3",
    "date-fns": "^2.8.1",
    "formik": "^2.0.8",
    "logrocket": "^1.0.5",
    "logrocket-react": "^4.0.1",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "reactn": "^2.2.4",
    "typescript": "~3.7.2",
    "yup": "^0.28.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test:watch": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint --ext ts,tsx ./src",
    "test": "react-scripts test --ci --watchAll=false --coverage",
    "test:coverage": "cat ./coverage/lcov.info | coveralls",
    "release": "standard-version"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@storybook/addon-actions": "^5.3.0-rc.1",
    "@storybook/addon-docs": "^5.2.8",
    "@storybook/addon-info": "^5.2.8",
    "@storybook/addon-links": "^5.3.0-rc.1",
    "@storybook/addon-storyshots": "^5.2.8",
    "@storybook/addon-viewport": "^5.2.8",
    "@storybook/addons": "^5.3.0-rc.1",
    "@storybook/preset-create-react-app": "^1.5.0",
    "@storybook/react": "^5.3.0-rc.1",
    "coveralls": "^3.0.9",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^3.1.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "require-context.macro": "^1.2.2",
    "standard-version": "^7.0.1",
    "storybook-react-router": "^1.0.8"
  },
  "jest": {
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
